{
	"name": "@ally-ui/react-dialog",
	"version": "0.0.2-alpha.0",
	"scripts": {
		"example": "vite",
		"dev": "concurrently \"tsc --watch\" \"vite build --watch\"",
		"build": "tsc && vite build",
		"preview": "vite preview"
	},
	"author": "Bryan Lee",
	"description": "A WAI-ARIA compliant Dialog for React",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ally-ui/ally-ui.git"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/main.umd.cjs",
	"module": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"exports": {
		".": {
			"import": "./dist/main.js",
			"require": "./dist/main.umd.cjs"
		}
	},
	"dependencies": {
		"@ally-ui/core-dialog": "*",
		"@ally-ui/react-utils": "*"
	},
	"peerDependencies": {
		"react": ">=16",
		"react-dom": ">=16"
	}
}
