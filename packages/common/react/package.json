{
	"name": "@ally-ui/react",
	"version": "0.0.11",
	"scripts": {
		"example": "vite",
		"build": "tsc && vite build",
		"build:watch": "vite build --watch",
		"clean": "rimraf dist"
	},
	"author": "Bryan Lee",
	"description": "React utilities for Ally UI",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ally-ui/ally-ui.git"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/main.umd.cjs",
	"module": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"exports": {
		".": {
			"import": "./dist/main.js",
			"require": "./dist/main.umd.cjs"
		},
		"./package.json": "./package.json"
	},
	"sideEffects": false,
	"peerDependencies": {
		"react": ">=16",
		"react-dom": ">=16"
	}
}
