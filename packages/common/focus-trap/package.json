{
	"name": "@ally-ui/focus-trap",
	"version": "0.0.6",
	"scripts": {
		"example": "vite",
		"build": "tsc && vite build",
		"build:watch": "concurrently \"tsc --watch\" \"vite build --watch\"",
		"test": "vitest run",
		"test:watch": "vitest",
		"coverage": "vitest run --coverage"
	},
	"author": "Bryan Lee",
	"description": "A tiny focus trap implementation for JavaScript",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ally-ui/ally-ui.git"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/main.umd.cjs",
	"module": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"exports": {
		".": {
			"import": "./dist/main.js",
			"require": "./dist/main.umd.cjs"
		}
	},
	"dependencies": {
		"@ally-ui/core": "^0.0.6"
	}
}
